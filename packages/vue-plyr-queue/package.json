{
	"name": "@mellkam/vue-plyr-queue",
	"version": "0.3.0",
	"module": "./dist/index.js",
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"type": "module",
	"description": "Plugin for @mellkam/vue-plyr that makes ease to create video/audio queue with plyr player.",
	"devDependencies": {
		"vue": "3.3.4",
		"plyr": "3.7.8",
		"vite": "4.4.4",
		"typescript": "5.1.6"
	},
	"peerDependencies": {
		"plyr": ">=3.6.3",
		"vue": ">=3.2.0"
	},
	"sideEffects": false,
	"scripts": {
		"build": "vite build && tsc"
	},
	"files": [
		"dist",
		"LICENSE",
		"package.json"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"types": "./dist/index.d.ts"
		}
	},
	"typesVersions": {
