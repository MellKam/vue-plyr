	"license": "MIT",
	"homepage": "https://github.com/MellKam/vue-plyr/blob/main/packages/vue-plyr-queue/README.md",
	"keywords": [
		"plyr",
		"player",
		"vue",
		"video",
		"audio",
		"youtube",
		"vimeo",
		"queue"
	],
	"author": {
		"name": "Artem Melnyk",
		"url": "https://github.com/MellKam"
	},
	"repository": {
		"type": "git",
