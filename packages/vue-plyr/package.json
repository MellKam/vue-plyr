{
	"name": "@mellkam/vue-plyr",
	"version": "0.2.0",
	"description": "Integration of the Plyr player with Vue framework",
	"type": "module",
	"main": "./dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./utils": {
			"import": "./dist/utils.js",
			"types": "./dist/utils.d.ts"
		}
	},
	"files": [
		"dist",
		"package.json",
		"src",
		"README.md"
	],
	"typesVersions": {
		"*": {
			"index": [
				"./dist/index.d.ts"
			],
			"utils": [
				"./dist/utils.d.ts"
			]
		}
	},
	"scripts": {
		"build": "vite build",
		"lint": "eslint . && prettier . -w"
	},
	"devDependencies": {
		"plyr": "^3.7.3",
		"typescript": "^4.9.3",
		"vite": "^3.2.4",
		"vite-plugin-dts": "^1.7.1",
		"vue": "^3.2.45"
	},
	"peerDependencies": {
		"plyr": ">=3.6.3",
		"vue": ">=3.2.0"
	},
	"peerDependenciesMeta": {
		"vue": {
			"optional": false
		},
		"plyr": {
			"optional": false
		}
	},
	"license": "MIT",
	"homepage": "https://github.com/MellKam/vue-plyr/blob/main/packages/vue-plyr/README.md",
	"keywords": [
		"plyr",
		"player",
		"vue",
		"video",
		"audio",
		"youtube",
		"vimeo"
	],
	"author": {
		"name": "Artem Melnyk",
		"url": "https://github.com/MellKam"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/MellKam/vue-plyr.git"
	}
}