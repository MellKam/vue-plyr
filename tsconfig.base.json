{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"preserveWatchOutput": true,
		"strictNullChecks": true,
		"strict": true,
		"lib": ["esnext", "dom"]
	},
	"exclude": ["node_modules/**", "dist/**"]
}
